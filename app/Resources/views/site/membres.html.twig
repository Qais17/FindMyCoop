{% extends ':site:home.html.twig' %}

{% block body %}
    <section>
        <div class="col-lg-offset-1 col-lg-6 , section">
            <h3 class="text-center">Membres</h3>
            <div>
                {% for membre  in membres %}
                    <img class="pull-left" src="http://icon-icons.com/icons2/877/PNG/128/male-profile-picture_icon-icons.com_68388.png" alt="">
                    <p>{{ membre.pseudo }}</p>
                    <p>{{ membre.mail }}</p>
                    <p>{{ membre.url }}</p>
                {% endfor %}
            </div>
    </section>
{% endblock %}
