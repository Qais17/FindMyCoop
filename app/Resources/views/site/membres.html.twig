{% extends ':site:home.html.twig' %}

{% block body %}
    <section>
        <div class="col-lg-8 , section">
            <h2 class="text-center">Membres</h2>
            <div>
                {% for membre  in membres %}
                    <img class="pull-left" src="{{ asset('image/avatar1.png') }}"/>
                    <p>{{ membre.pseudo }}</p>
                    <p>{{ membre.mail }}</p>
                    <p>{{ membre.url }}</p>
                {% endfor %}
            </div>
    </section>
{% endblock %}
